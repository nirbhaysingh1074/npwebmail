/* style sheets */
.chat_input,.new_overflow{ position:relative;height:100%;width: 258px;}
.chat_input_new {
  position: absolute !important;
  bottom: -14px !important;
  text-decoration: none;
}
..ui-corner-all {
  width: 10px !important;
  /* float: left !important; */
}
.ui-icon-minusthick {
  /* background-position: -64px -128px; */
  width: 15px;
  height: 24px;
 /*  background: #ccc; */
  display: block;
  text-align: center;  font-size: 38px;
  line-height: 18px;
}
.chat_minu {
  float: right;
  margin-left: 3px;text-decoration: none;
}
.ui-chatbox {
    /* position: fixed; */
    position: relative;
     bottom: 22px;
    padding: 2px;
    /* background:  #CCCCCC; */
    z-index: 9999;
}

.ui-chatbox-titlebar {
    cursor: pointer;
    height: 24px;
    line-height: 22px;
	font-weight: normal;
    padding: 3px;
}

.ui-chatbox-content {
    padding: 0px;
    margin: 0px;
    border: 0px;
}

.ui-chatbox-log {
    padding: 3px;
    height: 200px;
    overflow-y: auto;
    overflow-x: hidden;
    background: #FFFFFF;
}

.ui-chatbox-input {
    padding: 3px;
    border-top: 1px solid grey;
    overflow: hidden;
}

.ui-chatbox-input-box {
    margin: 5px;
	font-size: 12px !important;
    border: 2px solid lightgrey;/* #6699FF */
    padding: 2px;
    height: 50px;
}

.ui-chatbox-icon {
    float: right;
}

.ui-chatbox-input-focus {
    border-color: #6699FF;
}

.ui-chatbox-msg {
    margin-bottom: -15px;
    margin-top: -6px;
    float: left;
    clear: both;
    /* Source: http://snipplr.com/view/10979/css-cross-browser-word-wrap */
    white-space: pre-wrap;      /* CSS3 */
    white-space: -moz-pre-wrap; /* Firefox */
    white-space: -pre-wrap;     /* Opera <7 */
    white-space: -o-pre-wrap;   /* Opera 7 */
    word-wrap: break-word;      /* IE */
	font-size:12px;
}
